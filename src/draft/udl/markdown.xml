[Description]
Adds syntax highlighting for Markdown

Visit http://arcticicestudio.com/Northem for more informations.


[Development]
Please report bugs and suggestions for improvement/language support at TODO or via email.


[Installation]
Via integrated GUI: "Language" -> "Define your language" -> "Import"
-->
<NotepadPlus>
  <UserLang name="Markdown" ext="md markdown" udlVersion="2.0">
    <Settings>
      <Global caseIgnored="yes" forceLineCommentsAtBOL="yes"/>
      <Prefix Keywords1="yes" Keywords2="yes" Keywords3="yes" Keywords4="yes"/>
    </Settings>
    <KeywordLists>
      <Keywords name="Comments" id="0">00 01 02 03&lt;!-- 04--&gt;</Keywords>
      <Keywords name="Operators1" id="5">&lt; &gt;</Keywords>
      <Keywords name="Operators2" id="6">* - +</Keywords>
      <Keywords name="Keywords1" id="16">http:// (http:// https:// (https:// (/ /</Keywords>
      <Keywords name="Keywords2" id="17">==== ---- #</Keywords>
      <Keywords name="Keywords3" id="18">** __</Keywords>
      <Keywords name="Keywords4" id="19">* _</Keywords>
      <Keywords name="Delimiters" id="24">00[ 01 02] 03` 04 05`</Keywords>
    </KeywordLists>
    <Styles>
      <WordsStyle name="DEFAULT" styleID="0" fgColor="E4E8E8" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="COMMENTS" styleID="1" fgColor="626262" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="LINE COMMENTS" styleID="2" fgColor="626262" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="NUMBERS" styleID="3" fgColor="A074C4" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="KEYWORDS1" styleID="4" fgColor="E4E8E8" bgColor="2A2A2B" fontStyle="4" nesting="0"/>
      <WordsStyle name="KEYWORDS2" styleID="5" fgColor="6897BB" bgColor="2A2A2B" fontStyle="1" nesting="0"/>
      <WordsStyle name="KEYWORDS3" styleID="6" fgColor="45C1D1" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="KEYWORDS4" styleID="7" fgColor="45C1D1" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="OPERATORS" styleID="12" fgColor="6897BB" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="DELIMITERS1" styleID="16" fgColor="E6CD69" bgColor="2A2A2B" fontStyle="0" nesting="0"/>
      <WordsStyle name="DELIMITERS2" styleID="17" fgColor="45C1D1" bgColor="2A2A2B" fontStyle="2" nesting="0"/>
    </Styles>
  </UserLang>
</NotepadPlus>
